{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Expo App",
      "type": "reactnativedirect",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "port": 8081,
      "sourceMaps": true,
      "sourceMapRenames": true,
      "trace": "verbose",
      "skipFiles": ["<node_internals>/**"],
      "sourceMapPathOverrides": {
        "[metro-project]/*": "${workspaceFolder}/*",
        "[metro-project]/node_modules/*": "${workspaceFolder}/node_modules/*",
        "webpack:///./*": "${workspaceFolder}/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack:///./*~*": "${workspaceFolder}/node_modules/*",
        "metro:///*": "${workspaceFolder}/*",
        "/[metro-project]/*": "${workspaceFolder}/*"
      }
    }
  ]
}

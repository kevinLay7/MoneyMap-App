{
  "name": "moneymap",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --dev-client --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android ",
    "ios": "expo run:ios ",
    "web": "expo start --web ",
    "lint": "expo lint",
    "build-ios-preview": "eas build -p ios --profile preview --local",
    "ios-local": "eas build --clear-cache -p ios --profile preview --local --non-interactive --output './moneymap.ipa' && ios-deploy --bundle moneymap.ipa",
    "ios-local-prod": "eas build --clear-cache -p ios --profile production --local --non-interactive --output './moneymap.ipa' && ios-deploy --bundle moneymap.ipa",
    "ios-deploy": "ios-deploy --bundle moneymap.ipa",
    "build-ios-production": "eas build -p ios --profile production",
    "build-android-preview": "eas build -p android --profile preview",
    "build-android-production": "eas build -p android --profile production",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate-api": "npx swagger-typescript-api generate -p 'http://localhost:3001/swagger/json'  -o ./api/gen --modular --axios --single-http-client --extract-enums && node scripts/fix-api-syntax.js",
    "device": "npx expo run:ios --device"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.1.1",
    "@expo/vector-icons": "^15.0.3",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.13.1",
    "dayjs": "^1.11.19",
    "expo": "~54.0.31",
    "expo-background-fetch": "^14.0.9",
    "expo-background-task": "~1.0.10",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.8",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-task-manager": "^14.0.9",
    "expo-web-browser": "~15.0.8",
    "lottie-react-native": "^7.3.4",
    "nativewind": "^4.2.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-async-storage": "^0.0.1",
    "react-native-auth0": "^5.1.0",
    "react-native-circular-progress": "^1.4.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-charts": "^1.4.70",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-nitro-modules": "^0.32.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-plaid-link-sdk": "^12.7.0",
    "react-native-quick-crypto": "^1.0.6",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.18"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@morrowdigital/watermelondb-expo-plugin": "^2.3.3",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.7.4",
    "reactotron-core-client": "^2.9.9",
    "reactotron-react-native": "^5.1.18",
    "typescript": "~5.9.2"
  },
  "private": true
